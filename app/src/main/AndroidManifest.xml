<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.METAcademymyapplication">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.MailActivity"></activity>
        <activity android:name=".ui.WrongActivity" />
        <activity android:name=".ui.LogoutActivity" />
        <activity android:name=".ui.SendActivity" />
        <activity android:name=".ui.ReciveActivity" />
        <activity android:name=".ui.LoginActivity" />
        <activity android:name=".ui.GOActivity" />
        <activity android:name=".ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>